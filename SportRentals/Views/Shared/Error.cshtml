@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2>


<h3 class="text-danger">Controller: @Model.ControllerName</h3>
<h3 class="text-danger">Action: @Model.ActionName</h3>
<h4 class="text-danger">@Model.Exception.ToString()</h4>