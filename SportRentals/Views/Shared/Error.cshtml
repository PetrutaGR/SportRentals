@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h5 class="text-danger">An error occurred while processing your request.</h5>

<h6 class="text-danger">Controller: @Model.ControllerName</h6>
<h6 class="text-danger">Action: @Model.ActionName</h6>
<h6 >@Model.Exception.ToString()</h6>